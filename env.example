# Environment Variables for Wanderlust Project Development
# Copy this file to .env.development and fill in your actual values

# Mapbox Configuration
MAPBOX_TOKEN=your_mapbox_access_token_here
MAP_TOKEN=your_mapbox_access_token_here

# MongoDB Configuration
ATLASDB_URL=mongodb://localhost:27017/wanderlust

# Session Secret
SECRET=your_session_secret_here

# Cloudinary Configuration (for image uploads)
CLOUD_NAME=your_cloudinary_cloud_name
CLOUD_API_KEY=your_cloudinary_api_key
CLOUD_API_SECRET=your_cloudinary_api_secret

# Node Environment
NODE_ENV=development

# Port Configuration
PORT=8080 








